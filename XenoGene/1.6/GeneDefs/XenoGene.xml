	<HediffDef ParentName="XenomorphtypeBase">
		<defName>XMT_SkeletonInjection</defName>
		<label>discoloration</label>
		<description>A discoloration spreads out from multiple injection sites on the body.</description>
		<makesSickThought>true</makesSickThought>
		<maxSeverity>1</maxSeverity>
		<initialSeverity>1</initialSeverity>
		<stages>
			<li>
				
			</li>
		</stages>
		<comps>
			<li Class="Xenomorphtype.HediffCompProperties_PerPartPromoter">
				<HediffPromotionsByPart>
					<li>
						<increaseSeverity>1</increaseSeverity>
						<hediffDef>XMT_Mesoskeleton</hediffDef>
					</li>
				</HediffPromotionsByPart>
			</li>
		</comps>
	</HediffDef>

